{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["standard-with-typescript", "pluging:prettier/recommended", "eslint:recommended", "pluging:@typescript-eslint/recommended"],
  "plugins": ["@typescript-eslint", "import", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
