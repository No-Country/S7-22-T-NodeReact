{
    "version": 2,
    "builds": [
      { "src": "dist/app.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/app.js"
      }
    ],
    "env": {
        "PORT":"@port",
        "PASS_DB":"@pass_db",
        "NAME_DB":"@name_db",
        "USERNAME_DB":"@username_db",
        "HOST_DB":"@host_db"
    }
}