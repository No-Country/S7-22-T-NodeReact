{
    "version": 2,
    "builds": [
      { "src": "dist/app.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/app.js"
      }
    ],
    "env": {
      "NAME_DB":"@name_db_3",
      "USERNAME_DB":"@username_db_3",
      "PASS_DB":"@pass_db_3",
      "HOST_DB":"@HOST_DB_3",
      "CONNECTION_URI":"@connection_uri_3",
      "JWT_SECRET":"@jwt_secret_3_add"
    }
}